{
  "manifest_version": 3,
  "name": "Compact JIRA",
  "description": "Compact JIRA",
  "action": {
      "default_popup": "index.html",
      "default_title": "Toggle features"
  },
  "version": "1.0",
  "permissions": [
      "storage",
      "scripting"
  ],
  "icons": {
    "16": "logo192.png",
    "48": "logo192.png",
    "128": "logo192.png"
  },
  "content_scripts": [
      {
          "matches": ["https://jira.storebrand.no/*"],
          "js": ["toggle-content.js"]
      }
  ],
  "background": {
      "service_worker": "background.js"
  }
}