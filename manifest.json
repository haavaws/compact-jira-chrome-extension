{
    "manifest_version": 3,
    "name": "Hello Extensions test",
    "description": "Base Level Extension",
    "action": {
        "default_popup": "hello.html",
        "default_icon": "hello_extensions.png"
    },
    "version": "1.0",
    "permissions": [
        "storage",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": ["https://jira.storebrand.no/*"],
            "js": ["toggle-content.js"]
        }
    ],
    "background": {
        "service_worker": "background.js"
    }
  }